{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["plugin:react/recommended", "standard"],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "plugins": ["react"],
    "rules": {
        "semi": 0,
        "indent": [1, 4],
        "react/prop-types": 0,
        "quotes": 0,
        "comma-dangle": 0,
        "prefer-const": 0,
        "multiline-ternary": 0,
        "no-prototype-builtins": 0,
        "array-callback-return": 0,
        "no-constant-condition": 0,
        "no-unescaped-entities": 0,
        "react/no-unescaped-entities": 0,
        "no-undef":0,
        "react/react-in-jsx-scope":0
    }
}
